type Post @key(fields: "id") @softDeleteModel {
  title: String! @index
  content: String!
  user_id: ID! 
  user: User! @belongsTo
}

extend type Query {
  postPage: [Post!]! @paginate

  posts(id: [ID!]! @in, user_id: ID! @neq): [Post!]! @find

  post(id: ID! @eq): Post! @first

  getPost(fuck: String!): Post!
}

